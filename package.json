{
  "name": "@osjs/xterm-application",
  "version": "1.1.16",
  "description": "OS.js v3 Xterm Application",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint index.js",
    "stylelint": "stylelint index.scss",
