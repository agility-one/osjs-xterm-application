    "sass-loader": "^8.0.0",
    "stylelint": "^12.0.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "xterm": "^3.14.5"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@osjs/eslint-config"
  },
  "stylelint": {
    "extends": "@osjs/stylelint-config"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {}
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
